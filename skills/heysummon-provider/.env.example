# HeySummon Provider Configuration
# Copy to .env and fill in your values

# Platform URL (cloud or self-hosted)
HEYSUMMON_BASE_URL=https://cloud.heysummon.ai

# Your provider API key (from dashboard → Users → your profile)
HEYSUMMON_API_KEY=hs_prov_your_key_here

# Chat ID for notifications (e.g. Telegram chat ID)
HEYSUMMON_NOTIFY_TARGET=your_chat_id
